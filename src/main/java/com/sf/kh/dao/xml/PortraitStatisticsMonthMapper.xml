<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sf.kh.dao.mapper.PortraitStatisticsMonthMapper" >
  <resultMap id="portraitStatisticsMonthResultMap" type="com.sf.kh.model.PortraitStatisticsMonth" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="type" property="type" jdbcType="INTEGER" />
    <result column="type_name" property="typeName" jdbcType="VARCHAR" />
    <result column="month" property="month" jdbcType="VARCHAR" />
    <result column="goods_num" property="goodsNum" jdbcType="BIGINT" />
    <result column="version" property="version" jdbcType="INTEGER" />
  </resultMap>
  <sql id="baseColumns" >
    id, type, type_name, month, goods_num, version
  </sql>
  
  <select id="getList" resultMap="portraitStatisticsMonthResultMap">
    select <include refid="baseColumns" /> from t_kh_delivery_month
   	<where>
   		<if test="type!=null">type = #{type}</if>
   		<if test="typeName!=null">and type_name = #{typeName}</if>
   		<if test="version!=null">and version = #{version}</if>
   	</where>
   	order by month asc
  </select>
  
</mapper>