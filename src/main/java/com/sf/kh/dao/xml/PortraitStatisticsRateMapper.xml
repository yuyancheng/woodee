<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sf.kh.dao.mapper.PortraitStatisticsRateMapper" >
  <resultMap id="portraitStatisticsRateResultMap" type="com.sf.kh.model.PortraitStatisticsRate" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="type" property="type" jdbcType="INTEGER" />
    <result column="type_name" property="typeName" jdbcType="VARCHAR" />
    <result column="rate_name" property="rateName" jdbcType="VARCHAR" />
    <result column="rate" property="rate" jdbcType="DECIMAL" />
    <result column="version" property="version" jdbcType="INTEGER" />
  </resultMap>
  <sql id="baseColumns" >
    id, type, type_name, rate_name, rate, version
  </sql>
  <select id="getList" resultMap="portraitStatisticsRateResultMap">
    select <include refid="baseColumns" /> from t_kh_delivery_rate
   	<where>
   		<if test="type!=null">type = #{type}</if>
   		<if test="typeName!=null">and type_name = #{typeName}</if>
   		<if test="version!=null">and version = #{version}</if>
   	</where>
   	order by rate desc
  </select>
</mapper>